{
  "version": 2,
  "builds": [
    {
      "src": "build/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/icons/(.*)", "dest": "/icons/$1" },
    { "src": "/manifest.json", "dest": "/manifest.json" },
    { "src": "/favicon.png", "dest": "/favicon.png" },
    { "src": "/flutter.js", "dest": "/flutter.js" },
    { "src": "/main.dart.js", "dest": "/main.dart.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
